        {
            "id": 2,
            "definition": {
                "title": "Rate Limited % by Descriptor (Threshold 80%)",
                "show_legend": true,
                "legend_layout": "vertical",
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "type": "timeseries",
                "requests": [
                    {
                        "formulas": [
                            {
                                "alias": "Rate Limited %",
                                "formula": "(query1 / (query2 + 0.001)) * 100"
                            }
                        ],
                        "queries": [
                            {
                                "name": "query1",
                                "data_source": "metrics",
                                "query": "sum:ratelimit.service.rate_limit.over_limit{cluster:dev-eks-cluster} by {descriptor_value,descriptor_key}.as_count()"
                            },
                            {
                                "name": "query2",
                                "data_source": "metrics",
                                "query": "sum:ratelimit.service.rate_limit.total_hits{cluster:dev-eks-cluster} by {descriptor_value,descriptor_key}.as_count()"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "palette": "orange",
                            "line_type": "solid",
                            "line_width": "normal"
                        },
                        "display_type": "line"
                    }
                ],
                "yaxis": {
                    "include_zero": true,
                    "scale": "linear",
                    "label": "%",
                    "max": "100"
                },
                "markers": [
                    {
                        "label": "80% threshold",
                        "value": "y = 80",
                        "display_type": "error dashed"
                    }
                ]
            },
            "layout": {
                "x": 6,
                "y": 6,
                "width": 6,
                "height": 4
            }
