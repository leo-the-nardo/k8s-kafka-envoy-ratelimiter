{
    "title": "Rate Limit Service Dashboard",
    "description": "Monitor rate limiting by descriptor",
    "widgets": [
        {
            "id": 83391668808077,
            "definition": {
                "title": "Percentage of miss-allowed (by envoy metrics)",
                "show_legend": true,
                "legend_layout": "vertical",
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "type": "timeseries",
                "requests": [
                    {
                        "formulas": [
                            {
                                "alias": "%",
                                "formula": "clamp_min(100 * ((query1 - 6000) / ((query1 - 6000) + query2)), 0)"
                            }
                        ],
                        "queries": [
                            {
                                "name": "query1",
                                "data_source": "metrics",
                                "query": "sum:ratelimit.service.rate_limit.within_limit{descriptor_key:gold_filter}.as_count()"
                            },
                            {
                                "name": "query2",
                                "data_source": "metrics",
                                "query": "sum:ratelimit.service.rate_limit.over_limit{descriptor_key:gold_filter}.as_count()"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "palette": "warm",
                            "line_type": "solid",
                            "line_width": "normal"
                        },
                        "display_type": "line"
                    }
                ],
                "yaxis": {
                    "include_zero": true,
                    "scale": "linear",
                    "label": "excess TPS"
                },
                "markers": [
                    {
                        "label": " 10% ",
                        "value": "y = 10",
                        "display_type": "error dashed"
                    },
                    {
                        "label": " 1% ",
                        "value": "y = 1",
                        "display_type": "warning dashed"
                    }
                ]
            },
            "layout": {
                "x": 0,
                "y": 0,
                "width": 6,
                "height": 2
            }
        },
